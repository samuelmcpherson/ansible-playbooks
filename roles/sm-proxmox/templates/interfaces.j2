auto vmbr0
iface vmbr0 inet static
        address 192.168.90.10/24
        gateway 192.168.90.2
        bridge-ports eno1
        bridge-stp off
        bridge-fd 0
        bridge-vlan-aware yes
        bridge-vids 2-4094

